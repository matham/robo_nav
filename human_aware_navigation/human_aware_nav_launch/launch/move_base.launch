<launch>

  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">   
    <rosparam file="$(find ca_robo_nav)/param/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find ca_robo_nav)/param/costmap_common_params.yaml" command="load" ns="local_costmap" />   
    <rosparam file="$(find ca_robo_nav)/param/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find ca_robo_nav)/param/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find ca_robo_nav)/param/global_planner_params_nav.yaml" command="load" />
    <param name="TBPlanner/dynamic_layers_plugin" value="dynamic_social_costmap::SocialLayers" />
    <rosparam file="$(find ca_robo_nav)/param/local_planner_params.yaml" command="load" />
    <!--<remap from="cmd_vel" to="navigation_velocity_smoother/raw_cmd_vel"/>-->
  </node>

</launch>
